  "version": "1.0.0",
  "description": "A file manager backend",
  "type": "module",
  "main": "src/index.js",
  "bugs": {
    "url": "https://github.com/microDotBuilder/file-manager-backend/issues"
  },
  "homepage": "https://github.com/microDotBuilder/file-manager-backend#readme",
  "scripts": {
